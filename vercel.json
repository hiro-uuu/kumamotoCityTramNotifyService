{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/cron",
      "dest": "/api/cron.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "25 7 * * *"
    }
  ]
}
